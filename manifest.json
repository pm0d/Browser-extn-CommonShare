{
	"background": {
		"persistent": false,
		"scripts": [ "background.js" ]
	},
	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "CommonShare"
	},
	"content_scripts": [
		{
			"css": [ "main.css" ],
			"js": [ "main.js" ],
			"matches": [ "http://10.249.110.236/dashboard/*" ]
		}
	],
	"description": "This extension is for injecting CSS/javascript into CommonShare",
	"icons": {
		"128": "icon.png"
	},
	"manifest_version": 2,
	"name": "CommonShare",
	"permissions": [ "tabs", "activeTab" ],
	"version": "1.0"
}
